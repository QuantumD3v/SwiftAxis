{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/ny",
      "destination": "/index.js"
    },
    {
      "source": "/api/v1/json1",
      "destination": "/index.js"
    },
    {
      "source": "/api/util/base64",
      "destination": "/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/github",
      "destination": "https://github.com/QuantumD3v"
    }
  ]
}
