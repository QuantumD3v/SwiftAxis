{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/index.js"
    },
    {
      "src": "/file/.*",
      "dest": "api/index.js"
    },
    {
      "src": "/github",
      "status": 301,
      "headers": { "Location": "https://github.com/QuantumD3v" }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
